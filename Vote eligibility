#include <iostream>
using namespace std;

int main(){
    int age,x,y;
    char a[10];
    
    
    cout<<"\nEnter your age : ";
    cin>>age;
    
    if(age>18)
        cout<<"\nEligible for voting\n";
        
    else if(age==18){
        cout<<"\nDo you have voter id\n";
        cin>>a;
        
        x = strcmp(a,"no");
        y = strcmp(a,"No");
        if(x==0||y==0)
            cout<<"\nApply for a voter id\n";
        else
            cout<<"\nEligible for voting\n";
        
    }
    else
        cout<< "\nNot eligible for voting\n";
}
